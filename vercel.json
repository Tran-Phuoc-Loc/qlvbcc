{
    "version": 2,
    "builds": [
      {
        "src": "**/*.php",
        "use": "@vercel/php"
      },
      {
        "src": "**/*.html",
        "use": "@vercel/static"
      },
      {
        "src": "style/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "image/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  